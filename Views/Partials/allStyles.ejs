<style>
body{
    background-color: #e1dada;
    color: #080728;
    margin: 0;
    margin-bottom: 10px;
    overflow-x: hidden;
}
div#form{
    display: block;
    text-align: center;
}
form{
    margin: 20px auto 20px auto;
    display: inline-block;
    text-align: left;
    flex: auto;
    width: fit-content;
    max-width: 99%;
    font-size: calc(15px + 0.390625vw);
    padding: 5px;
    .buttons{
        border: 2px solid black;
        margin-right: 10%;
    }
}

label, input, textarea{
    font-size: calc(15px + 0.390625vw);

}
input, textarea{
    padding: 4px;
    border: 2px solid black;
    margin-left: 4px;
}
textarea{
    padding: 10px;
    width: 90%;
    margin-bottom: 10px;
}
fieldset{
    background-color: white;
    padding: 20px;
}
legend{
    background-color: black;
    color: white;
    padding: 5px;
}
header{
    font-size: 24px;
    text-decoration: underline;
    text-align: center;
    margin: 20px 0 5px 0;
}
.para{
    font-size: calc(15px + 0.390625vw);
    width: 99%;
}
.container{
    display: grid;
    margin-top: 20px;
    row-gap: 15px;
    column-gap: 5px;
    grid-template: max-content/max-content max-content max-content;
}
a{
    color: white;
}
@media screen and (min-width:1400px){
    .container{
        grid-template-columns: max-content max-content max-content max-content;
    }
}
@media screen and (min-width:1800px){
    .container{
        grid-template-columns: max-content max-content max-content max-content max-content;
    }
}
@media screen and (min-width:2100px){
    .container{
        grid-template-columns: max-content max-content max-content max-content max-content max-content;
    }
}
@media screen and (min-width:2400px){
    .container{
        grid-template-columns: max-content max-content max-content max-content max-content max-content max-content;
    }
}
@media screen and (max-width:1000px){
    .container{
        grid-template-columns: max-content max-content;
    }
}
@media screen and (max-width:800px){
    .container{
        grid-template-columns: max-content;
    }
}
h1{
    margin-top: 15px;
}

.projectBox {
    width: 300px;
    height: 250px;
    margin-right: 5px;
    border: 2px solid #008a57;
    background-color: rgb(255, 255, 255);
    position: relative; /* Add this to contain the inner element */
    
    ::-webkit-scrollbar {
        width: 5px;
        height: 5px;
    }
    ::-webkit-scrollbar-track {
        box-shadow: inset 0 0 5px grey;
        border-radius: 10px;
    }
    
    ::-webkit-scrollbar-thumb {
        background: rgb(255, 255, 255);
        border-radius: 5px;
    }
}
@media screen and(max-width:500px) {
    .projectBox{
        width: 150px;
    }
}
.done{
    background-color: #3fffb9;
    color: #001f3f;
}

/* Adjusting direction for scrollbar positioning */
.notes {
    max-height: 130px;
    font-size: 18px;
    padding: 10px;
    overflow-y: auto;
    direction: rtl; /* Set the container to right-to-left direction */
}

.notes{
    direction: ltr; /* Set the content back to left-to-right direction */
}

/* Title Styles */
.title {
    max-height: 40px;
    max-width: 100px;
    font-size: 20px;
    white-space: nowrap; /* Prevent text wrapping */
    overflow-x: auto; /* Enable horizontal scrolling if needed */
    overflow-y: hidden; /* Prevent vertical scrolling */
    padding: 5px;
    box-sizing: border-box; /* Include padding and border in element's total width and height */
    text-overflow: ellipsis; /* Show ellipsis for overflowed text */
    max-width: 100%; /* Ensure it doesn't exceed the container's width */
    border-bottom: 3px solid white; /* Optional: border style */
}



#pen, #delete{
    width: 40px;
    height: 40px;
    border-radius: 20px;
    bottom: 0;
    margin: 5px 0% 2% 0px;
    text-align: center;
    background: none; /* Remove default button background */
    border: none; /* Remove default button border */
    cursor: pointer; /* Change cursor to pointer to indicate it's clickable */
    outline: none; /* Remove outline on focus */
}
#pen{
    right: 0;
}
#delete{
    left: 0;
}
.wrapper .sidebar ul#list {
    flex-grow: 1;
    overflow: auto;
    max-height: 90%;
    padding-bottom: 0;
    scrollbar-width: thin;
    scrollbar-color: #888 #f1f1f1;
    direction: rtl; /* Scrollbar on left */
}

.wrapper .sidebar ul#list li {
    direction: ltr;
    padding: 15px;
    border-bottom: 1px solid rgba(0, 0, 0, 0.05);
    border-top: 1px solid rgba(255, 255, 255, 0.05);
}

.wrapper .sidebar ul#list::-webkit-scrollbar {
    width: 8px;
}

.wrapper .sidebar ul#list::-webkit-scrollbar-track {
    background: #f1f1f1;
}

.wrapper .sidebar ul#list::-webkit-scrollbar-thumb {
    background-color: #888;
    border-radius: 10px;
    border: 2px solid #f1f1f1;
}

.wrapper .sidebar ul#list::-webkit-scrollbar-thumb:hover {
    background-color: #555;
}
@media screen and (min-width: 600px) {
    .blur {
        filter: blur(5px);
        pointer-events: none; /* Prevent clicking outside */
    }
}

.material-symbols-outlined.out {
    font-size: 50px;
    color: #3fffb9;
    border-radius: 30%;
    opacity: 0.9;
    background-color: black;
}

.material-symbols-outlined:hover{
    opacity: 1.0;
}

span.small{
    font-size: 30px;
}
.delete:hover, .tick:hover{
    cursor: pointer;
}
.delete.out, .tick.out{
    background-color: black;
    border: 2px solid white;
    position: absolute;
    bottom: 0;
    right: 0;
    margin-bottom: 10px;
    margin-right: 5px;
}
li a{
    text-align: left;
}
.tick{
    margin-right: 60px;
}
.due-date{
    bottom: 0;
    left: 0;
    position: absolute;
    margin-bottom: 10px;
    font-size: 20px;
    margin-left: 10px;
}



@import url('https://fonts.googleapis.com/css?family=Josefin+Sans&display=swap');

*{
  margin: 0;
  padding: 0;
  box-sizing: border-box;
  list-style: none;
  text-decoration: none;
  font-family: 'Josefin Sans', sans-serif;
}
.wrapper{
    display: flex;
}

.wrapper .sidebar {
    width: 140px;
    height: 100vh; /* Full viewport height */
    background: #4b4276;
    padding: 30px 0;
    position: fixed;
    display: flex;
    flex-direction: column;
    justify-content: space-between; /* Distribute space between the top and bottom sections */
  }
  
  #first {
    /* This section stays at the top */
  }
  
  #second {
    margin-top: auto; /* Pushes the second section to the bottom */
  }
  
  #second li {
    position: static;
    display: flex;
    font-size: 20px;
    padding: 15px;
    border-bottom: 1px solid #bdb8d7;
    border-bottom: 1px solid rgba(0,0,0,0.05);
    border-top: 1px solid rgba(255,255,255,0.05);
  }
  
.wrapper .sidebar h2{
    color: #fff;
    text-align: center;
    margin-bottom: 30px;
}
  
.wrapper .sidebar ul li{
    padding: 15px;
    border-bottom: 1px solid #bdb8d7;
    border-bottom: 1px solid rgba(0,0,0,0.05);
    border-top: 1px solid rgba(255,255,255,0.05);
}    

.wrapper .sidebar ul li a{
    display: block;
    font-size: 20px;
}
  
.wrapper .sidebar ul li a .fas{
    width: 25px;
}
  
.wrapper .sidebar ul li:hover{
    background-color: #594f8d;
}
      
a{
    color: white;
    background-color: transparent;
    border: 0;
}
   
.wrapper .sidebar .social_media{
    position: absolute;
    bottom: 0;
    left: 50%;
    transform: translateX(-50%);
    display: flex;
    margin-bottom: 10px;
    flex-shrink: 0;
}

  
  
main#content {
    min-width: 270px;
    width: calc(100% - 150px); /* Subtract the left margin to avoid overflow */
    margin-left: 150px;
    margin-top: 20px;
    padding-right: 20px;
    box-sizing: border-box; /* Include padding and borders in the element's total width */
  }
  

.wrapper #content .header{
    padding: 20px;
    background: #fff;
    color: #717171;
    border-bottom: 1px solid #e0e4e8;
}
  
.wrapper #content .info{
    margin: 20px;
    color: #717171;
    line-height: 25px;
}
  
.wrapper #content .info div{
    margin-bottom: 20px;
}
h2{
    font-size: calc(14px + 0.390625vw);
}
.wrapper .sidebar .social_media a{
    display: block;
    width: 35px;
    background: #594f8d;
    height: 40px;
    line-height: 45px;
    text-align: center;
    font-size: large;
    padding-top: 10px;
    margin: auto 5px;
    color: #bdb8d7;
    border-top-left-radius: 5px;
    border-top-right-radius: 5px;
}

#extend{
    position: static;
    margin-top: auto; 
    display: flex;
    font-size: 20px;
    padding: 15px;
    border-bottom: 1px solid #bdb8d7;
    border-bottom: 1px solid rgba(0,0,0,0.05);
    border-top: 1px solid rgba(255,255,255,0.05);
}
#create:hover,#extend:hover{
    background-color: #594f8d;
}
ul#list{
    overflow: auto;
    overflow-x: auto;
}

